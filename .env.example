# Example environment variables for vcom_yuman_sync.
# This file is for reference only and should not be committed with real secrets.

# VCOM API credentials
VCOM_API_KEY=your_api_key_here
VCOM_USERNAME=your_username_here
VCOM_PASSWORD=your_password_here

# Yuman API
YUMAN_TOKEN=your_yuman_token_here

# Supabase (primary data store)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# Legacy PostgreSQL connection (used by some workflows)
DATABASE_URL=postgresql://user:password@localhost:5432/dbname

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
